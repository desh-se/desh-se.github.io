---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Anders Berggren's blog</title>
	<link href="https://desh.se/blog/atom.xml" rel="self"/>
	<link href="https://desh.se/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://desh.se/blog</id>
	<author>
		<name>Anders Berggren</name>
		<email>anders@desh.se</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://desh.se{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://desh.se{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>